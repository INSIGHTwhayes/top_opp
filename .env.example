# =============================================================================
# RelTracker Environment Configuration
# =============================================================================
# Copy this file to .env and fill in the values
# NEVER commit .env to git (it contains secrets)
# =============================================================================

# PostgreSQL Configuration
POSTGRES_USER=reltracker
POSTGRES_PASSWORD=your_secure_password_here
POSTGRES_DB=reltracker

# n8n Configuration
N8N_ENCRYPTION_KEY=generate_a_random_32_plus_character_string_here
N8N_USER_MANAGEMENT_DISABLED=true

# Timezone (for n8n scheduled workflows)
GENERIC_TIMEZONE=America/New_York

# Future API Keys (add as needed)
# LINKEDIN_API_KEY=
# PITCHBOOK_API_KEY=
# HUNTER_API_KEY=
